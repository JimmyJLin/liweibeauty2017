<div id="menus" class="container-fluid">

  <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
    <div id="membership" class="row">
      <div class="col-xs-18 col-sm-12 col-md-12">
        <div class="panel panel-default">
          <div class="panel-heading" role="tab" id="headingOne">
            <h4 class="panel-title">
              <a class="accordion-toggle" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                Membership Packages
              </a>
            </h4>
          </div>
          <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
            <div class="panel-body">
              <div class="container-fluid">
                  <% data.packages.forEach((el) => { %>
                    <div class="items">
                      <h4><%= el.title %> <span> - <%= el.price %> (<%= el.length%>)</span></h4>
                      <p><%= el.summary %></p>
                    </div>
                  <% }) %>
              </div>
            </div>
          </div>
        </div>
      </div>

    </div>

    <div class="row">
      <div id="left-panel" class="col-xs-18 col-sm-6 col-md-6">
        <div class="panel panel-default">
          <div class="panel-heading" role="tab" id="headingTwo">
            <h4 class="panel-title">
              <a class="accordion-toggle" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                Facial Services
              </a>
            </h4>
          </div>
          <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
            <div class="panel-body">
              <div class="panel-body">
                <div class="container-fluid">
                    <% data.facial.forEach((el) => { %>
                      <div class="items">
                        <h4><%= el.title %> <span> - <%= el.price %> (<%= el.length%>)</span></h4>
                        <p><%= el.summary %></p>
                      </div>
                    <% }) %>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="panel panel-default">
          <div class="panel-heading" role="tab" id="headingThree">
            <h4 class="panel-title">
              <a class="accordion-toggle" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
