                  <% }) %>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="col-xs-18 col-sm-6 col-md-6">

        <div class="panel panel-default">
          <div class="panel-heading" role="tab" id="headingFive">
            <h4 class="panel-title">
              <a class="accordion-toggle" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
                GM Collin Services
              </a>
            </h4>
          </div>
          <div id="collapseFive" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingFive">
            <div class="panel-body">
              <div class="container-fluid">
                  <% data.collin.forEach((el) => { %>
                    <div class="items">
                      <h4><%= el.title %> <span> - <%= el.price %> (<%= el.length%>)</span></h4>
                      <p><%= el.summary %></p>
                    </div>
                  <% }) %>
              </div>
            </div>
          </div>
        </div>

        <div class="panel panel-default">
          <div class="panel-heading" role="tab" id="headingSix">
